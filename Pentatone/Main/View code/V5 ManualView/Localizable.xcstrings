    "manual.welcome.description" : {
      "comment" : "Description of what Pentatone is",
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Pentatone is a musical instrument based on the Arithmophone, a keyboard designed to explore just intonation through pentatonic scales."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Pentatone es un instrumento musical basado en el Arithmophone, un teclado diseñado para explorar la entonación justa a través de escalas pentatónicas."
          }
        },
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Pentatone ist ein Musikinstrument, das auf dem Arithmophone basiert, einer Tastatur zur Erforschung der reinen Stimmung mittels pentatonischer Tonleitern."
          }
        },
        "fr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Pentatone est un instrument de musique basé sur l'Arithmophone, un clavier conçu pour explorer l'intonation juste à travers des gammes pentatoniques."
          }
        },
        "ja" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Pentatoneは、ペンタトニック・スケールを通じて純正律を探求するために設計されたキーボード、Arithmophoneに基づいた楽器です。"
          }
        }
      }
    },
