    "manual.keyboard.touch.content" : {
      "comment" : "Explanation of touch sensitivity feature",
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "The keyboard responds to touch position and pressure. Slide your finger horizontally across a key to add vibrato. Touch harder for more expression."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "El teclado responde a la posición y presión del toque. Desliza tu dedo horizontalmente sobre una tecla para añadir vibrato. Presiona más fuerte para mayor expresión."
          }
        },
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Die Tastatur reagiert auf Berührungsposition und Druck. Gleiten Sie horizontal über eine Taste, um Vibrato hinzuzufügen. Drücken Sie fester für mehr Ausdruck."
          }
        },
        "fr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Le clavier répond à la position et à la pression du toucher. Glissez votre doigt horizontalement sur une touche pour ajouter du vibrato. Appuyez plus fort pour plus d'expression."
          }
        },
        "ja" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "キーボードはタッチの位置と圧力に反応します。キーを横にスライドしてビブラートをかけることができます。より強く押すと、より表現豊かになります。"
          }
        }
      }
    }
